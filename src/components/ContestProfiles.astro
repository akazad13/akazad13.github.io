---
import Container from "./Container.astro";
import { Icon } from "astro-icon/components";
---

<div class="text-gray-600" id="contest-profile">
    <Container>
        <div class="mb-14 space-y-4 px-6 md:px-0">
            <h2 class="text-center text-2xl font-bold text-gray-100 md:text-4xl">Contest Profiles</h2>
        </div>
        <div class="divide-gray-100 overflow-hidden rounded-3xl border border-gray-100 text-gray-600 dark:divide-gray-700 dark:border-gray-700">
            <div class="relative bg-white dark:bg-gray-800">
                <div class="relative space-y-4 p-8">
                    <div class="relative mb-10 flex justify-center gap-4">
                        <img src="./images/application.png" class="w-12" alt="" />
                        <h5 class="mt-2 text-2xl font-semibold text-gray-200 transition">Problems that I Set</h5>
                    </div>

                    <div class="mt-10 flex flex-wrap justify-center gap-x-6 gap-y-4 text-gray-200">
                        <table class="w-full">
                            <thead>
                                <tr class="border-b">
                                    <th class="p-4 pl-0 pt-0 text-left">Problem Title </th>
                                    <th class="p-4 pt-0 text-left"> First Appearance </th>
                                    <th class="p-4 pt-0 text-left"> Category </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>Twin</strong></td>
                                    <td class="p-4">Coding Contest 21, DevSkill</td>
                                    <td class="p-4">Math, Combinations and Permutations</td>
                                </tr>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>You Know Who?!!!!!!</strong></td>
                                    <td class="p-4">RUET জ্ঞানJam 2018</td>
                                    <td class="p-4">Matrix Exponential, Math</td>
                                </tr>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>Boro vai ... trt chai..!!</strong></td>
                                    <td class="p-4">Coding Contest 21, DevSkill</td>
                                    <td class="p-4">Math, Number Theory</td>
                                </tr>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>Friendly game</strong></td>
                                    <td class="p-4">-</td>
                                    <td class="p-4">Basic Math</td>
                                </tr>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>Special Trt</strong></td>
                                    <td class="p-4">Comming Soon</td>
                                    <td class="p-4">Number Theory, Moduler Arithmetic</td>
                                </tr>
                                <tr class="border-b">
                                    <td class="p-4 pl-0"><strong>Lord of the Rings </strong></td>
                                    <td class="p-4">Comming Soon</td>
                                    <td class="p-4">Graph Theory</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="relative bg-white dark:bg-gray-800">
                <div class="relative space-y-4 p-8">
                    <div class="relative mb-10 flex justify-center gap-4">
                        <img src="./images/brain.png" class="w-12" width="512" height="512" alt="" />

                        <h5 class="mt-2 text-2xl font-semibold text-gray-200 transition">Online Judge Profiles</h5>
                    </div>
                    <div class="mt-10 flex flex-wrap justify-center gap-x-6 gap-y-4 text-gray-200">
                        <a
                            href="https://codeforces.com/profile/Mirror"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> Codeforces <sup> 471</sup></span>
                        </a>

                        <a
                            href="https://uhunt.onlinejudge.org/id/549708"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> UVA <sup> 292</sup></span>
                        </a>

                        <a
                            href="https://lightoj.com/user/akazad-cse13"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> LightOJ <sup> 215</sup></span>
                        </a>

                        <a
                            href="https://leetcode.com/u/akazad13/"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> Leetcode <sup> 137</sup></span>
                        </a>

                        <a
                            href="https://www.hackerrank.com/profile/akazad_ruet13"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> HacckerRank <sup> 45</sup></span>
                        </a>

                        <a
                            href="https://toph.co/u/akazad.ruet13"
                            target="_blank"
                            class="inline-flex cursor-pointer items-center gap-2 rounded-full bg-primary px-6 py-3 text-base font-semibold text-white transition-all ease-in-out hover:scale-105 active:scale-100 disabled:pointer-events-none disabled:opacity-50"
                        >
                            <Icon name="streamline:star-badge" class:list={["text-2xl"]} />
                            <span class="relative text-base font-semibold text-white"> Toph <sup> 51</sup></span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </Container>
</div>
